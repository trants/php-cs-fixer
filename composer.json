{
  "name": "vnspc/php-cs-fixer",
  "description": "Automatically configuration PHP CS Fixer for projects.",
  "keywords": [
    "php",
    "php-cs-fixer",
    "trants",
    "vspc"
  ],
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Son Tran Thanh",
      "email": "286.trants@gmail.com",
      "homepage": "https://opensource.trants.me"
    }
  ],
  "require": {
    "php": "^8.0",
    "friendsofphp/php-cs-fixer": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Vnspc\\PhpCsFixer\\": "src"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.0.x-dev"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
